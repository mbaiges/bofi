{
  "name": "trading-bot-system",
  "version": "1.0.0",
  "description": "A comprehensive trading bot system with API, frontend, and bot services",
  "private": true,
  "workspaces": [
    "api",
    "frontend",
    "bot"
  ],
  "scripts": {
    "dev:api": "cd api && npm start",
    "dev:frontend": "cd frontend && npm start",
    "dev:bot": "cd bot && npm start",
    "dev:all": "concurrently \"npm run dev:api\" \"npm run dev:bot\"",
    "install:all": "npm install && cd api && npm install && cd ../bot && npm install",
    "start": "npm run dev:api"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "trading",
    "bot",
    "tradingview",
    "candlesticks",
    "indicators"
  ],
  "author": "",
  "license": "MIT"
}